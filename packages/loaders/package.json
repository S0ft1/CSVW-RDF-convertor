{
  "name": "@csvw-rdf-convertor/loaders",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "sourceRoot": "packages/loaders/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "packages/loaders/dist",
          "main": "packages/loaders/src/index.ts",
          "tsConfig": "packages/loaders/tsconfig.lib.json",
          "stripLeadingPaths": true
        }
      }
    },
    "name": "@csvw-rdf-convertor/loaders"
  },
  "dependencies": {
    "@csvw-rdf-convertor/core": "0.5.0",
    "@rdfjs/prefix-map": "^0.1.2",
    "@rdfjs/serializer-turtle": "^1.1.5",
    "@rdfjs/types": "^2.0.1",
    "@swc/helpers": "~0.5.11",
    "jsonld-streaming-parser": "^5.0.0",
    "jsonld-streaming-serializer": "^4.0.0",
    "n3": "^1.26.0",
    "rdfxml-streaming-parser": "^3.1.0",
    "readable-stream": "^4.7.0"
  }
}
