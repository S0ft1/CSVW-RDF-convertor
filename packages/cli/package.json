{
  "name": "@csvw-rdf-convertor/cli",
  "version": "0.0.3",
  "description": "",
  "keywords": [
    "csv",
    "csvw",
    "rdf",
    "convert",
    "converter",
    "conversion",
    "stream",
    "streaming"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/S0ft1/CSVW-RDF-convertor"
  },
  "homepage": "https://s0ft1.github.io/CSVW-RDF-convertor",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "bin": "dist/index.js",
  "nx": {
    "sourceRoot": "packages/cli/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "packages/cli/dist",
          "main": "packages/cli/src/index.ts",
          "tsConfig": "packages/cli/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true
        }
      }
    },
    "tags": [
      "env:cli"
    ]
  },
  "dependencies": {
    "@csvw-rdf-convertor/core": "0.0.3",
    "@inquirer/prompts": "^7.3.1",
    "@swc/helpers": "~0.5.11",
    "chalk": "^5.4.1",
    "fuzzysort": "^3.1.0",
    "n3": "^1.24.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.1",
    "@types/yargs": "^17.0.33"
  }
}
