{
  "name": "@csvw-rdf-convertor/vscode",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "displayName": "CSVWRDFConvertor",
  "description": "",
  "engines": {
    "vscode": "^1.102.0"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "sourceRoot": "packages/vscode/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "packages/vscode/dist",
          "main": "packages/vscode/src/index.ts",
          "tsConfig": "packages/vscode/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true
        }
      }
    }
  },
  "dependencies": {
    "@swc/helpers": "~0.5.11",
    "@csvw-rdf-convertor/core": "0.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "csvwrdfconvertor.showInputFields",
        "title": "Add Conversion",
        "icon": "$(add)"
      },
      {
        "command": "csvwrdfconvertor.convert",
        "title": "Convert",
        "icon": "$(gear)"
      },
      {
        "command": "csvwrdfconvertor.deleteConversion",
        "title": "Delete Conversion",
        "icon": "$(trash)"
      },
      {
        "command": "csvwrdfconvertor.addAnotherInput",
        "title": "Add another input",
        "icon": "$(add)"
      },
      {
        "command": "csvwrdfconvertor.convertCurrentWindow",
        "title": "Convert Current Window",
        "icon": "$(gear)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "csvw-rdf-converter",
          "title": "CSVW RDF Converter",
          "icon": "$(file-code)"
        }
      ]
    },
    "views": {
      "csvw-rdf-converter": [
        {
          "id": "csvw-rdf-actions",
          "name": "Actions",
          "type": "tree"
        }
      ]
    },
    "menus": {
      "editor/title": [
        {
          "command": "csvwrdfconvertor.convertCurrentWindow",
          "when": "resourceExtname == .csv || resourceExtname == .rdf || resourceExtname == .ttl || resourceExtname == .jsonld",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "csvwrdfconvertor.deleteConversion",
          "when": "view == csvw-rdf-actions && viewItem == conversion",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2"
  }
}